{
    "version": "0.1.0",
    "driver": {
        "serialization": "json",
        "exe": "python -m pbsvtools.tasks.make_reports --resolved-tool-contract ",
        "env": {}
    },
    "schema_version": "2.0.0",
    "tool_contract": {
