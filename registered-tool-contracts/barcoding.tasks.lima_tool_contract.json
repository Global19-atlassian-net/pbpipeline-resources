                "title": "SubreadSet"
            },
            {
                "description": "Barcode DataSet or .fasta file",
                "file_type_id": "PacBio.DataSet.BarcodeSet",
                "id": "barcode_set",
                "title": "BarcodeSet"
            }
        ],
        "is_distributed": true,
        "name": "lima",
        "nproc": "$max_nproc",
        "output_types": [
            {
                "default_name": "lima_output",
