{
    "version": "1.0.1.TAG-1470",
    "driver": {
        "serialization": "json",
        "exe": "python -m pbtranscript.tasks.ice_quiver_postprocess --resolved-tool-contract",
        "env": {}
    },
    "schema_version": "2.0.0",
    "tool_contract": {
        "task_type": "pbsmrtpipe.task_types.standard",
        "resource_types": [
            "$tmpdir"
        ],
        "description": "\nCalls the ICE algorithm, which stands for 'Iteratively Clustering and Error\ncorrection', to identify de novo consensus isoforms.\n",
        "schema_options": [
            {
                "optionTypeId": "float",
                "default": 0.99,
                "id": "pbtranscript.task_options.hq_quiver_min_accuracy",
                "name": "Minimum Quiver|Arrow Accuracy",
                "description": "Minimum allowed Quiver|Arrow accuracy to classify an isoform as hiqh-quality (default: 0.99)"
            },
            {
                "optionTypeId": "integer",
                "default": 100,
                "id": "pbtranscript.task_options.qv_trim_5p",
                "name": "Trim QVs 5'",
                "description": "Ignore QV of n bases in the 5' end (default 100)."
            },
            {
                "optionTypeId": "integer",
                "default": 30,
                "id": "pbtranscript.task_options.qv_trim_3p",
                "name": "Trim QVs 3'",
                "description": "Ignore QV of n bases in the 3' end (default 30)."
            }
        ],
        "output_types": [
            {
                "title": "Transcript Clustering Report",
                "description": "JSON summary",
                "default_name": "summary",
                "id": "json_summary",
                "file_type_id": "PacBio.FileTypes.json"
            },
            {
                "title": "Clustering Results",
                "description": "Clustering results for each CCS read",
                "default_name": "cluster_report",
                "id": "cluster_report",
                "file_type_id": "PacBio.FileTypes.csv"
            },
            {
                "title": "High-Quality Isoforms",
                "description": "Isoforms with high consensus accuracy",
                "default_name": "hq_isoforms",
                "id": "hq_isoforms_fa",
                "file_type_id": "PacBio.DataSet.ContigSet"
            },
            {
                "title": "High-Quality Isoforms",
                "description": "Isoforms with high consensus accuracy",
                "default_name": "hq_isoforms",
                "id": "hq_isoforms_fq",
                "file_type_id": "PacBio.FileTypes.Fastq"
            },
            {
                "title": "Low-Quality Isoforms",
                "description": "Isoforms with low consensus accuracy",
                "default_name": "lq_isoforms",
                "id": "lq_isoforms_fa",
                "file_type_id": "PacBio.DataSet.ContigSet"
            },
            {
                "title": "Low-Quality Isoforms",
                "description": "Isoforms with low consensus accuracy",
                "default_name": "lq_isoforms",
                "id": "lq_isoforms_fq",
                "file_type_id": "PacBio.FileTypes.Fastq"
            }
        ],
        "_comment": "Created by pbcommand 0.6.5",
        "name": "pbtranscript.tasks.ice_quiver_postprocess",
        "input_types": [
            {
                "description": "A FOFN of bam or dataset xml (e.g., input.fofn|bam|subreadset.xml), which contain quality values of raw reads and subreads",
                "title": "SubreadSet",
                "id": "subreads_fofn",
                "file_type_id": "PacBio.DataSet.SubreadSet"
            },
            {
                "description": "ContigSet of consensus isoforms",
                "title": "Consensus isoforms",
                "id": "consensus_fa",
                "file_type_id": "PacBio.DataSet.ContigSet"
            },
            {
                "description": "Cluster pickle file",
                "title": "Clusters",
                "id": "cluster_pickle",
                "file_type_id": "PacBio.FileTypes.pickle"
            },
            {
                "description": "Pickle file for non-full-length read mapping",
                "title": "Pickle file",
                "id": "map_nofl_pickle",
                "file_type_id": "PacBio.FileTypes.pickle"
            },
            {
                "description": "Sentinel file from ice_quiver task",
                "title": "JSON file",
                "id": "json_in",
                "file_type_id": "PacBio.FileTypes.json"
            }
        ],
        "nproc": "$max_nproc",
        "is_distributed": true,
        "tool_contract_id": "pbtranscript.tasks.ice_quiver_postprocess"
    },
    "tool_contract_id": "pbtranscript.tasks.ice_quiver_postprocess"
}
